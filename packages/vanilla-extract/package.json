{
  "name": "@codeimage/vanilla-extract",
  "version": "0.2.0",
  "description": "Vanilla extract customizations for @codeimage styles",
  "main": "./dist/index.cjs",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "rollup -c"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "node": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./vite-plugin": {
      "import": "./dist/vite-plugin/index.mjs",
      "default": "./dist/vite-plugin/index.mjs",
      "node": "./dist/vite-plugin/index.cjs",
      "require": "./dist/vite-plugin/index.cjs",
      "types": "./dist/types/vite-plugin/index.d.ts"
    }
  },
  "keywords": [],
  "author": "Riccardo Perra",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "18.11.0",
    "@vanilla-extract/integration": "^6.0.1",
    "esbuild": "^0.19.8",
    "outdent": "^0.8.0",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "rollup": "^2.79.1",
    "typescript": "~5.3.2"
  },
  "dependencies": {
    "@vanilla-extract/integration": "6.0.1",
    "outdent": "0.8.0",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "vite": "^3.2.5"
  }
}