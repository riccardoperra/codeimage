    "solid-headless": "^0.10.12",
    "solid-use": "^0.3.3",
    "csstype": "^3.0.11",
    "clsx": "^1.1.1",
    "@floating-ui/dom": "^0.4.4",
    "@floating-ui/core": "^0.6.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "rimraf": "^3.0.2",
    "solid-js": "^1.3.13",
    "typescript": "^4.6.2",
    "vite": "^2.9.1",
    "vite-plugin-dts": "^1.0.5",
    "vite-plugin-solid": "^2.2.6",
    "esbuild": "^0.14.31",
    "tiny-glob": "^0.2.9",
    "esbuild-plugin-solid": "^0.4.2"
  }
