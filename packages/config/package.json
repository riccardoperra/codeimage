{
  "name": "@codeimage/config",
  "version": "0.0.1",
  "module": "./dist/config.es.js",
  "source": "./src/public-api.ts",
  "types": "./dist/src/public-api.d.ts",
  "exports": {
    ".": {
      "import": "./dist/config.es.js",
      "types": "./dist/src/public-api.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm clean && vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "pre-commit": "lint-staged --relative",
    "pre-commit-prettier": "prettier --write",
    "pre-commit-eslint": "eslint --ext .js,.jsx,.mjs,.ts,.tsx -c ../../.eslintrc.js --fix",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx -c ../../.eslintrc.js",
    "clean": "rimraf dist",
    "build-esm": "tsc -p tsconfig.esm.json",
    "build-dts": "tsc -p tsconfig.dts.json",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json",
    "dev": "chokidar \"src/**/*.{ts,tsx}\" -c \"pnpm build\""
  },
  "dependencies": {
    "solid-js": "^1.3.7",
    "@codeimage/theme": "workspace:*",
    "@codemirror/state": "^0.19.9",
    "@codemirror/view": "^0.19.45",
    "@codemirror/lang-css": "^0.19.3",
    "@codemirror/lang-html": "^0.19.4",
    "@codemirror/lang-java": "^0.19.1",
    "@codemirror/lang-javascript": "^0.19.7",
    "@codemirror/lang-json": "^0.19.2",
    "@codemirror/lang-php": "^0.19.1",
    "@codemirror/lang-python": "^0.19.4",
    "@codemirror/lang-cpp": "^0.19.1",
    "@codemirror/lang-rust": "^0.19.1",
    "@codemirror/lang-markdown": "^0.19.6",
    "@codemirror/lang-sql": "^0.19.4"
  },
  "peerDependencies": {
    "solid-js": "^1.3.7",
    "@codeimage/theme": "workspace:*",
    "@codemirror/state": "^0.19.9",
    "@codemirror/view": "^0.19.45"
  },
  "devDependencies": {
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-solid": "^2.2.5",
    "rimraf": "^3.0.2"
  }
}
